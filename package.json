{
	"private": true,
	"type": "commonjs",
	"scripts": {
		"build": "npm run build:11ty",
		"build:11ty": "npx eleventy",
		"build:rollup": "npx rollup -c rollup.config.mjs",
		"clean": "rm -rf dist/ && rm -rf node_modules/",
		"dev": "npm run build:rollup && npm run dev:11ty",
		"dev:11ty": "npx eleventy --serve",
		"dev:rollup": "npx rollup -c rollup.config.mjs -w"
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-react": "^7.23.3",
		"@johnsiddoway/sass": "^1.0.5",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.5",
		"@rollup/plugin-terser": "^0.4.4",
		"markdown-it-attrs": "^4.1.6",
		"markdown-it-highlightjs": "^4.0.1",
		"normalize.css": "^8.0.1",
		"react": "^18.2.0",
		"react-color-palette": "^7.1.0",
		"react-dom": "^18.2.0",
		"rollup": "^4.9.5",
		"sass": "^1.63.6"
	}
}
