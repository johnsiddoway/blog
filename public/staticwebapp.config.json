{
	"globalHeaders": {
		"content-security-policy": "default-src https:",
		"Strict-Transport-Security": "max-age=15768000",
		"X-Frame-Options": "DENY"
	},
	"responseOverrides": {
		"404": {
			"rewrite": "/404.html"
		}
	},
	"navigationFallback": {
		"rewrite": "index.html",
		"exclude": [
			"/assets/*",
			"/robots.txt",
			"/sitemap.xml"
		]
	},
	"mimeTypes": {
		".json": "text/json"
	}
}